55555

n=int(input())
for i in range(1,n+1):
    for j in range(1,i+1):
        print(i,end=" ")
    print()

### reverse of a number, palindrome or not

n=int(input('Enter number'))
rev=0
temp=n
while n!=0:
    rem=n%10
    rev=rev*10+rem
    n=n//10
if rev is temp:
    print('palindrome')
else:
    print('not a palindrome')

